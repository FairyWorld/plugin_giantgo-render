{
  "name": "giantgo-render",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "main": "./packages/dist/giantgo-render.umd.js",
  "module": "./packages/dist/giantgo-render.es.js",
  "exports": {
    ".": {
      "import": "./packages/dist/giantgo-render.es.js",
      "require": "./packages/dist/giantgo-render.umd.js"
    }
  },
  "scripts": {
    "dev": "pnpm -C play dev",
    "build": "pnpm -C packages/giantgo-render build",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GiantGo/giantgo-render.git"
  },
  "keywords": [
    "giantgo-render",
    "form-generator",
    "vite",
    "vue3"
  ],
  "author": "spx443812507",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GiantGo/giantgo-render/issues"
  },
  "homepage": "https://github.com/GiantGo/giantgo-render",
  "peerDependencies": {
    "vue": "^3.2.16"
  },
  "dependencies": {
    "@giantgo-render/components": "workspace:*",
    "@giantgo-render/utils": "workspace:*",
    "codemirror": "^5.63.3",
    "element-plus": "^1.1.0-beta.19",
    "mitt": "^3.0.0",
    "quill": "^1.3.7",
    "vue": "^3.2.16",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.16",
    "autoprefixer": "^10.3.6",
    "sass": "~1.32.13",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.6.4",
    "vite-svg-loader": "^2.2.0"
  }
}
