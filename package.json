{
  "private": true,
  "packageManager": "pnpm@6.24.4",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "build": "pnpm -C packages/giantgo-render build",
    "example:dev": "pnpm -C example dev",
    "example:build": "pnpm -C example build",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "peerDependencies": {
    "vue": "^3.2.16"
  },
  "dependencies": {
    "@giantgo-render/components": "workspace:*",
    "@giantgo-render/utils": "workspace:*",
    "china-area-data": "^5.0.1",
    "codemirror": "^5.63.3",
    "element-plus": "^1.3.0-beta.4",
    "giantgo-render": "workspace:*",
    "mitt": "^3.0.0",
    "quill": "^1.3.7",
    "signature_pad": "^4.0.0",
    "vue": "^3.2.16",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.16",
    "autoprefixer": "^10.3.6",
    "sass": "^1.43.3",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.6.4",
    "vite-svg-loader": "^2.2.0"
  }
}
