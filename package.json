{
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "build": "pnpm -C packages/giantgo-render build",
    "example:dev": "pnpm -C example dev",
    "example:build": "pnpm -C example build",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@giantgo-render/components": "workspace:*",
    "@giantgo-render/hooks": "workspace:*",
    "@giantgo-render/theme": "workspace:*",
    "@giantgo-render/utils": "workspace:*",
    "@wangeditor/editor": "^5.1.14",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ace-builds": "^1.8.1",
    "axios": "^0.23.0",
    "china-area-data": "^5.0.1",
    "element-plus": "2.2.12",
    "giantgo-render": "workspace:*",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "signature_pad": "^4.0.0",
    "vue": "^3.2.37",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.89",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.3",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.3",
    "vite": "^3.0.0"
  }
}
